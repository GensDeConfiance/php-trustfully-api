{
    "name": "gens-de-confiance/php-trustfully-api",
    "type": "library",
    "description": "Trustfully API client",
    "keywords": ["trustfully", "api"],
    "license": "MIT",
    "authors": [{
        "name": "Gens de Confiance",
        "homepage": "https://github.com/GensDeConfiance"
